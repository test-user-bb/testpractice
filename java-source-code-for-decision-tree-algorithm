<script type="text/javascript">

    $(function () {
        $.ajax({
            url: '/banner/GetPost/' + '0' + '/?count=9' + '&showTaxonomy=true',
            type: 'GET',
            async: false,
            dataType: 'json',
            success: function (result) {

                if (result.Success) {

                    drawCarousel(result.Banners);

                    $('.carousel').carousel({
                        interval: 5500
                    });

                    setTimeout(HideBanner, 5000);
                }
                else {
                    $('#carouselSlider').hide();
                    $('#banner-728').show();
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $('#carouselSlider').hide();
                $('#banner-728').show();
            }
        });
    });

    function HideBanner() {
        $('#carouselSlider').show();
        $('#banner-728').hide();
    }

    function drawCarousel(banners) {
        var temp;
        var imageUrl = 'http://images.kdkeys.net';
        temp = [];
        var item_position = 0;
        var index_closed = true;

        $.each(banners, function (index, val) {
            //Add Tab
            //Add Content
            item_position = index;

            if (index == 0 || index == 3 || index == 6) {
                temp.push('<div id="fragment-' + index + '" class="item">');
                temp.push(' <ul class="thumbnails">');
                index_closed = false;
            }

            temp.push('     <li class="span4">');
            temp.push('         <div class="thumbnail"><img src="' + imageUrl + val.MediumImageURL + '" alt="' + val.FullImageTitle + '" title="' + val.FullImageTitle + '" style="max-height:90px;" />');

            temp.push('         <div>');
            temp.push('             <h5><a href="' + val.URL + '" >' + val.FullImageTitle + '</a></h5>');
            temp.push('         </div>');

            temp.push('         </div>');

            temp.push('     </li>');

            if (index == 2 || index == 5 || index == 8) {
                temp.push(' </ul>');
                temp.push('</div>');
                index_closed = true;
            }
        });

        if (index_closed == false) {
            temp.push(' </ul>');
            temp.push('</div>');
        }

        $('#carouselSlider .carousel-inner').append(temp.join(''));
    }


</script>


