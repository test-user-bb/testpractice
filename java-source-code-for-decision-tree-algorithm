        $(function () {
            var newsletter = '0'
            var id = '1381';

            if (Number(newsletter) == 1) {
                $.post("/Post/GetEmailNewsletter", { id: id },
                        function (obj) {
                            var emailId = obj.EmailNewsletterID;
                            if (Math.round(emailId) == Number(emailId) && Number(emailId) > 0) {
                                $.post('Post/ShowEmailNewsletter/' + emailId, function (data) {
                                    $('#EmailNewsletterSection').replaceWith(data);
                                });
                            }
                        }
                );
            }
            else {

            }
        });     
    </script>

