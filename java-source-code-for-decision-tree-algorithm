<script type="text/javascript">

    $(document).ready(function () {
        var dataUrl = 'http://www.kdkeys.net/' + 'Account/GetMemberLoginView';
        $.ajax({
            type: "post",
            dataType: "json",
            traditional: true,
            url: dataUrl,
            headers: { "cache-control": "no-cache" },
            data: $('<form><input name="__RequestVerificationToken" type="hidden" value="lRo0pa-4hRhekQa5Vi3eU1pzrz24gTAT7DAh1DgMWqArb_CfGBdSntZ-UYrY35V-aD7Nx4_ooQU_uHaJ_Jp41mPbP20PR41VSXZU-eJVHoJ73wqdJZdGHQm_Py6whIg-P6_j5GWA1xVHNfadmsyQwLQSAlrKCijupTM5JkGrSqE1" /></form>').serialize(),
            success: function (result) {
                if (result.Success) {
                    $("#Account_UserName").text(result.Success_Data.UserName);
                    showLogon(false);
                }
                else {
                    showLogon(true);
                }
            }            ,
            error: function () {
                showLogon(true);
            }
        });
    });

    function showLogon(display) {
        if (display) {
            $("#Account_UserName").hide();
            $("#Account_LogOff").hide();
            $("#Account_LogOn").show();
        }
        else {
            $("#Account_UserName").show();
            $("#Account_LogOff").show();
            $("#Account_LogOn").hide();
        }
    }
</script>

                
