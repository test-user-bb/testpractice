<script src="/Content/scripts/mvc.jquery.extends.js" type="text/javascript"></script>
<script type="text/javascript">

    $(function () {
        $('#SubmitYourQuestionButton').on('click', function () {
            if ($('#AskAQuestionForm').valid()) {

                $('#AskAQuestionModal').block({ baseZ: 1000, allowBodyStretch: true, timeout: 20000, cursor: 'wait', css: { width: '100%', backgroundColor: '#EEE' }, message: '<div class="loadwait"><img src="http://www.kdkeys.net/content/images/loading.gif" /> Please Wait ...</div>' });
                var data = $('#AskAQuestionForm').serialize();
                var dataUrl = 'http://www.kdkeys.net/' + 'Post/AskAQuestion';

                var heading = 'Ask A Queston';
                var question_error = 'An error occured and your message was not submitted. Please contact the administrator or try again later!';
                var cancelButtonTxt = 'Cancel';
                var okButtonTxt = 'OK';
                var callbackOK = function () {
                    //do nothing
                    $("#AskAQuestionModal").modal("hide");
                };
                var callbackCancel = function () {
                    //do nothing
                    $("#AskAQuestionModal").modal("hide");
                };

                $.post(dataUrl, data, function (result) {
                    if (result.Success) {
                        clearForm($('#AskAQuestionForm'), [])

                        ShowOKCancelModal(heading, result.Success_Message, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                    }
                    else {
                        ShowOKCancelModal(heading, question_error, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                    }

                });
                $.unblockUI({ fadeOut: 2000 });

            }
        });
    });

</script>
<!-- end scripts at the footer for delayed loading -->

