    <script type="text/javascript">
        $(function () {
            $(".star").mouseover(function () {
                var span = $(this).parent("span");
                var newRating = $(this).attr("value");
                setRating(span, newRating);
            });

            $(".star").mouseout(function () {
                var span = $(this).parent("span");
                var rating = span.attr("rating");
                setRating(span, rating);
            });


            $(".star").click(function () {
                var span = $(this).parent("span");
                var newRating = $(this).attr("value");
                var postID = span.attr("post");
                var text = span.children("span");
                $.post("/Post/SetRating", { id: postID, rating: newRating },
                        function (obj) {
                            if (obj.Success) {
                                text.html("Rating: " + obj.Result.Rating + " (" + obj.Result.Raters + " votes cast)"); //modify the text
                                span.attr("rating", obj.Result.Rating); //set the rating attribute
                                setRating(span, obj.Result.Rating); //update the display
                                //alert("Thank you, your vote was casted successfully.");
                            }
                            else {
                                //alert(obj.Message); //failure, show message
                            }
                        }
                );
            });

        });

        function setRating(span, rating) {
            span.find('img').each(function () {
                var value = parseFloat($(this).attr("value"));
                var imgSrc = $(this).attr("src");
                if (value <= rating)
                    $(this).attr("src", imgSrc.replace("-off.gif", "-on.gif"));
                else
                    $(this).attr("src", imgSrc.replace("-on.gif", "-off.gif"));
            });
        }
    </script>
    <script type="text/javascript">
        function ReplyPost(parentPostID, replyPostID, postTitle) {

            $('.enter_the_verification_word').hide();
            $(".enter_the_verification_word").css("display", "none");

            $("#SubmitAReplySection #ParentPostID").val(parentPostID);
            $("#SubmitAReplySection #ReplyPostID").val(replyPostID);
            $("#SubmitAReplySection #Title").val(postTitle);

            $("#SubmitAReplySection #oper").val("add");

            $("#SubmitAReplySection").show();
            $('#highlight-SubmitAReply').hide();
            //$('#SubmitAReplySection').unblock();
            return false;
        }

        function ReplyPostAsJson(id) {

            $('.enter_the_verification_word').hide();
            $(".enter_the_verification_word").css("display", "none");

            var dataUrl = 'http://www.kdkeys.net/' + 'Post/GetReplyAsJson/' + id;
            $.get(dataUrl, function (data) {
                PushSubmitAReplyData(data);
            });

            $("#SubmitAReplySection").show();
            $('#highlight-SubmitAReply').hide();
            //$('#SubmitAReplySection').unblock();
            return false;
        }

        function PushSubmitAReplyData(data) {
            $('.enter_the_verification_word').hide();
            for (record in data) {
                $("#SubmitAReplySection #" + record).val(data[record]);
            }
            $("#SubmitAReplySection #oper").val("add");
        }

        $(function () {
            $('#SubmitAReplyButton').on('click', function () {

                $('.enter_the_verification_word').hide();
                $(".enter_the_verification_word").css("display", "none");

                if ($('#SubmitAReplyForm').valid()) {

                    $('#SubmitAReplySection').block({ baseZ: 1000, allowBodyStretch: true, timeout: 20000, cursor: 'wait', css: { width: '100%', backgroundColor: '#EEE' }, message: '<div class="loadwait"><img src="http://www.kdkeys.net/content/images/loading.gif" /> Please Wait ...</div>' });
                    var heading = 'Post A Reply';
                    var question_error = 'An error occured and your message was not submitted. Please contact the administrator or try again later!';
                    var cancelButtonTxt = 'Cancel';
                    var okButtonTxt = 'OK';

                    var callbackOK = function () {
                        $("#SubmitAReplySection").hide();
                    };
                    var callbackCancel = function () {
                        $("#SubmitAReplySection").hide();
                    };


                    var data = $('#SubmitAReplyForm').serialize();
                    var dataUrl = 'http://www.kdkeys.net/' + 'Post/SubmitAReplyAsJson';
                    $.post(dataUrl, data, function (result) {
                        if (result.Success) {
                            clearForm($('#SubmitAReplyForm'), []);
                            ShowOKCancelModal(heading, result.Success_Message, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                            $("#SubmitAReplySection").hide();
                        }
                        else {
                            ShowOKCancelModal(heading, question_error, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                        }
                    });

                    $.unblockUI({ fadeOut: 2000 });
                }
                else {
                    //$('.enter_the_verification_word').show();
                }
            });
        });

        $(document).ready(function () {
            $('.tiny_mce').each(function () {
                var input = '#' + this.id;
                var count = 'div[id*="' + this.id + '_count_' + '"]'; 
                word_count(input, count);
                $(this).keyup(function () { word_count(input, count) });
            });
        });

        function word_count(field, count) {

            var number = 0;
            var matches = $(field).val().match(/\b/g);

            if (matches) {
                number = matches.length / 2;
            }

            $(count).text(field.replace('#', '') + ' : ' + number + ' Word' + (number != 1 ? 's' : '') + ' ');
        }


