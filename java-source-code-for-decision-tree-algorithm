        <!-- begin handle search form -->
        <script type="text/javascript">
            $("#searchform").submit(function () {
                var isFormValid = false;

                $(".required input").each(function () {
                    if ($(this).val() === undefined) {
                        isFormValid = false;
                    }
                    else if ($(this).val().length == 0) {
                        isFormValid = false;
                    }
                    else if ($(this).val().length > 3 && $(this).val() != 'Search...') {
                    }

                    if (!isFormValid) {
                        $(this).switchClass("", "highlight", 2000, "easeInOutQuad");
                    }
                    else {
                        $(this).switchClass("highlight", "", 2000, "easeInOutQuad");
                    }
                });

                return isFormValid;
            });
        </script>
        <!-- end  handle search form -->
