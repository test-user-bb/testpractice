        /** BEGIN ADD A REFERENCE LINK **/
        function AddReference(id){    
                $('#AddAReferenceSection').show();
                return false;
            } //end function


     $(function () {
            $('#AddAReferenceButton').on('click', function () {
                if ($('#AddAReferenceForm').valid()) {

                    $('#AddAReferenceSection').block({ baseZ: 1000, allowBodyStretch: true, timeout: 20000, cursor: 'wait', css: { width: '100%', backgroundColor: '#EEE' }, message: '<div class="loadwait"><img src="http://www.kdkeys.net/content/images/loading.gif" /> Please Wait ...</div>' });
                    var heading = 'Add A Reference';
                    var question_error = '<h4>An error occured and your reference / link was not submitted.</h4>';
                    var cancelButtonTxt = 'Cancel';
                    var okButtonTxt = 'OK';

                    var callbackOK = function () {
                        $("#AddAReferenceSection").hide();
                    };
                    var callbackCancel = function () {
                        //$("#AddAReferenceSection").hide();
                    };


                    var data = $('#AddAReferenceForm').serialize();
                    var dataUrl = 'http://www.kdkeys.net/' + 'Post/AddAReferenceLink';
                    $.post(dataUrl, data, function (result) {
                        if (result.Success) {
                            clearForm($('#AddAReferenceForm'), ['TopicTitle','HostName','PostDate','LinkID','PostId','PermaLinkId']);
                            ShowOKCancelModal(heading, result.Success_Message, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                            $("#AddAReferenceSection").hide();
                        }
                        else {
                            
                            if(result.errors.length > 0)
                            {
                                question_error += '<ul>';

                                for (var i = 0; i < result.errors.length; i++) {
                                    question_error += '<li> ' + result.errors[i] + ' </li>';
                                }

                                question_error += '</ul>';
                            }

                            ShowOKCancelModal(heading, question_error, cancelButtonTxt, okButtonTxt, callbackOK, callbackCancel);
                        }
                    });

                    $.unblockUI({ fadeOut: 1000 });
                }
                else {
                    //$('.enter_the_verification_word').show();
                }
            });
        });

        /** BEGIN STYLES **/
        $(document).ready(function() {
                   var faqColor = 'faqColorGreen';
                $(".lead h3, .comment-feeditem h3").each(function(i) {
                    var index = String(i + 1);
                    if (index.charAt(0)=='1') faqColor = 'faqColor'; else if (index.charAt(0)=='2') faqColor = 'faqColorBlue'; else if (index.charAt(0)=='3') faqColor = 'faqColorGreen';
                    else if (index.charAt(0)=='4') faqColor = 'faqColorGrey'; else if (index.charAt(0)=='5') faqColor = 'faqColorLightCoral';
                    else if (index.charAt(0)=='6') faqColor = 'faqColorMoccasin'; else if (index.charAt(0)=='7') faqColor = 'faqColorNavy';
                    else if (index.charAt(0)=='8') faqColor = 'faqColorBlack'; else if (index.charAt(0)=='9') faqColor = 'faqColorIvory';
                    else if (index.charAt(0)=='10') faqColor = 'faqColorLightSeaGreen'; else faqColor = faqColor;

                    $(this).html('<span class="faqCounter ' + faqColor + '">' + index + '</span>' + $(this).text());
                });
              });
        /** END STYLES **/
